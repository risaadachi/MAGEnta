<h1>Codes#edit</h1>

<%= form_with model: @code, local: true do |f| %>
<%= f.file_field :photos, multiple: true %><br>
<%= f.text_field :title, :placeholder => "Title" %><br>
<%= f.text_area :body, :placeholder => "Explanation" %><br>

<%= f.text_field :tagname, :placeholder => "Tag" %><br>

<%= f.submit %>
<% end %>